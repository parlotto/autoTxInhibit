# set idVendor idProduct and serial according to your TS590 and USB/RS2332 adapter
# you can change port names ts590 and ts590RS232 if you want 
SUBSYSTEM=="tty", ATTRS{idVendor}=="10c4", ATTRS{idProduct}=="ea60", ATTRS{serial}=="xxxxxxxxx", SYMLINK+="ts590"
SUBSYSTEM=="tty", ATTRS{idVendor}=="0403", ATTRS{idProduct}=="6001", ATTRS{serial}=="xxxxxxxxx", SYMLINK+="ts590RS232"

